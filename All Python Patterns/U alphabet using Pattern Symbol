n=5
for i in range(1,n+1):  
    for j in range(1,n+1):
        if ((j == 1 or j == n) and i != n) or (i == n and j != 1 and j != n):
            print('*',end=' ')
        else:
            print(' ',end=' ')
    print(end = ' ')
    print()
print()

### OUTPUT ###

*       *  
*       *  
*       *
*       *
  * * *
