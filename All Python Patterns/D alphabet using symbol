n=5

for x in range(1,n+1):
    for y in range(1,n+1):
        if (y==1 or ((x==1 or x==n ) and y<=n-1)):
            print("*",end="")
        elif(x!=1 and x!=n and y==n):
            print("*",end="")
        else:
            print(" ",end="")
    print()
    
    ### OUTPUT ###

    ****
    *   *
    *   *
    *   *
    ****